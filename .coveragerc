[run]
branch = True
source = 
    src
omit =
    tests/*
    docs/*
    tools/*
    scripts/*
    */migrations/*
    */generated/*
    */vendor/*
    */third_party/*
    setup.py
    */__main__.py
    src/*/experimental/*
    src/*/legacy/*

[report]
skip_covered = True
show_missing = True
precision = 1